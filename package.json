{
  "name": "rjv-react-antd",
  "version": "1.0.0-alpha.3",
  "description": "Ant Design's form fields powered by the rjv-react",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "repository": "https://github.com/gromver/rjv-react-antd",
  "author": "Gayazov Roman <gromver5@gmail.com>",
  "license": "MIT",
  "files": [
    "dist",
    "src",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "info": "npm-scripts-info",
    "storybook": "start-storybook",
    "storybook:docs": "start-storybook --docs",
    "cleanup": "shx rm -rf dist package",
    "prebuild": "npm run cleanup",
    "build": "gulp build",
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --project .",
    "test": "yarn lint && jest",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "changelog": "standard-version",
    "prerelease": "npm run test",
    "release": "standard-version",
    "postrelease": "npm run build && npm run release:github && npm run release:npm",
    "release:github": "git push --no-verify --follow-tags origin master",
    "release:npm": "npm publish",
    "reset": "git clean -dfx && git reset --hard && yarn"
  },
  "scripts-info": {
    "info": "Display information about the scripts",
    "build": "(Trash and re)build the library",
    "lint": "Lint all typescript source files",
    "test": "Lint and test the library",
    "test:watch": "Watch test files, rerun relevant tests",
    "test:coverage": "Run tests, generate the coverage report",
    "changelog": "Bump package.json version, update CHANGELOG.md, tag a release",
    "reset": "Delete all untracked files and reset the repo to the last commit",
    "release": "Test, bump package.json version, update CHANGELOG.md, tag a release, clean, build, push release to github and publish to npm"
  },
  "engines": {
    "node": ">=4.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "maxSubjectLength": 120
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": "npm run lint",
      "pre-push": "npm test -- --bail --onlyChanged"
    }
  },
  "standard-version": {
    "skip": {
      "changelog": true
    }
  },
  "keywords": [
    "antd",
    "rjv",
    "react",
    "rx",
    "validation",
    "field",
    "form",
    "json",
    "schema",
    "json-schema"
  ],
  "jest": {
    "rootDir": "src",
    "coverageDirectory": "../coverage",
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "dependencies": {},
  "peerDependencies": {
    "antd": "^4.0.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "rjv": "^5.0.0-alpha.8",
    "rjv-react": "^5.0.0-alpha.14"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@storybook/react": "^6.0.28",
    "@types/es6-promise": "^0.0.33",
    "@types/invariant": "^2.2.29",
    "@types/jest": "^26.0.10",
    "@types/lodash": "^4.14.160",
    "@types/node": "^14.6.0",
    "@types/react-test-renderer": "^16.9.3",
    "@types/storybook__react": "^5.2.1",
    "antd": "^4.8.4",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "babel-preset-react-app": "^9.1.2",
    "commitizen": "^4.2.0",
    "cz-conventional-changelog": "^3.2.1",
    "gulp": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^5.0.1",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lodash": "^4.17.20",
    "merge2": "^1.4.1",
    "npm-scripts-info": "^0.3.9",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.8.6",
    "rjv": "^5.0.0-alpha.8",
    "rjv-react": "^5.0.0-alpha.14",
    "shx": "^0.3.2",
    "standard-version": "^8.0.2",
    "ts-jest": "^26.4.4",
    "tslib": "^2.0.3",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^4.0.5",
    "validate-commit-msg": "^2.14.0"
  }
}
